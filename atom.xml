<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>timm.io - Timm Preetz</title>
 <link href="http://www.timm.io/atom.xml" rel="self"/>
 <link href="http://www.timm.io/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://www.timm.io/</id>
 <author>
   <name>Timm Preetz</name>
   <email>timm@preetz.us</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://www.timm.io{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://www.timm.io{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>
